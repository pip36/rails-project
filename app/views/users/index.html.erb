<h1> All Users </h1>
<% @users.each do |user| %>
<ul class="users">
      <li>
      <%= gravatar_for user, size: 50 %>
      <%= link_to user.name, user %> |
      <%if current_user.admin? && !current_user?(user)%>
        <%= link_to "Delete User", user, method: :delete, data: {confirm: "Are you sure?"} %>
      <%end%>
    </li>
</ul>
<% end %>
